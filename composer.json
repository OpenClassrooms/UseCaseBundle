{
    "name": "openclassrooms/use-case-bundle",
    "description": "Symfony2 Bundle for OpenClassrooms UseCase",
    "type": "symfony-bundle",
    "keywords": ["design"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@turn-it-up.org"
        },
        {
            "name": "OpenClassrooms",
            "email": "tech@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenClassrooms\\Bundle\\UseCaseBundle\\": ""
        }
    },
    "require": {
        "php": ">=7.1",
        "openclassrooms/use-case": "1.0.*@dev",
        "openclassrooms/cache-bundle": "2.0.*@dev",
        "openclassrooms/cache": "1.0.*@dev",
        "symfony/dependency-injection": "~3.4 || ~4.0",
        "symfony/config": "~3.4 || ~4.0",
        "symfony/http-kernel": "~3.4 || ~4.0",
        "symfony/security": "~3.4 || ~4.0",
        "doctrine/orm": "~2.3",
        "symfony/event-dispatcher": "~3.4 || ~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7",
        "satooshi/php-coveralls": "~2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
