{
    "name": "openclassrooms/use-case-bundle",
    "description": "Symfony2 Bundle for OpenClassrooms UseCase",
    "type": "symfony-bundle",
    "keywords": ["design"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@turn-it-up.org"
        },
        {
            "name": "OpenClassrooms",
            "email": "tech@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenClassrooms\\Bundle\\UseCaseBundle\\": ""
        }
    },
    "require": {
        "php": ">=8.2",
        "openclassrooms/use-case": "dev-remove_openclassrooms_cache",
        "symfony/cache":  "~5.4 || ~6.4 || ~7.1",
        "symfony/dependency-injection": "~5.4 || ~6.4 || ~7.1",
        "symfony/config": "~5.0 || ~6.4 || ~7.1",
        "symfony/http-kernel": "~5.0 || ~6.4 || ~7.1 || ~7.1",
        "symfony/security-core": "~5.0 || ~6.4 || ~7.1",
        "doctrine/orm": "~2.3 || ~3.2",
        "symfony/event-dispatcher": "~5.0 || ~6.4 || ~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
